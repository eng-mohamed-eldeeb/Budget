<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<% if !user_signed_in?  %>
  <script type="text/javascript">
    window.location.href="/splashs/index"  // put your correct path in a string here
  </script>
<% end %>
<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", users_path %>
    </p>
  <% end %>
</div>

